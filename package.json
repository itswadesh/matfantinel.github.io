{
  "name": "swadesh-behera-website",
  "version": "0.0.1",
  "scripts": {
    "dev": "svelte-kit dev --host",
    "build": "rm -rf build && svelte-kit build && touch build/.nojekyll",
    "deploy": "npm run build && npx gh-pages -d build -t true",
    "preview": "svelte-kit preview",
    "lint": "prettier --ignore-path .gitignore  --check --plugin-search-dir=. . && eslint --ignore-path .gitignore .",
    "format": "prettier --ignore-path .gitignore --write --plugin-search-dir=. ."
  },
  "devDependencies": {
    "@contentful/rich-text-html-renderer": "^15.13.1",
    "@sveltejs/adapter-static": "^1.0.0-next.34",
    "@sveltejs/kit": "1.0.0-next.357",
    "eslint": "^8.19.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-svelte3": "^4.0.0",
    "gh-pages": "^4.0.0",
    "install": "^0.13.0",
    "mdsvex": "^0.10.6",
    "npm": "^8.13.2",
    "prettier": "~2.7.1",
    "prettier-plugin-svelte": "^2.7.0",
    "reading-time": "^1.4.0",
    "rehype-autolink-headings": "^6.1.0",
    "rehype-external-links": "^1.0.0",
    "rehype-slug": "^5.0.0",
    "sass": "^1.53.0",
    "svelte": "^3.48.0",
    "svelte-preprocess": "^4.10.7",
    "svelte-sitemap": "^2.5.1"
  },
  "type": "module",
  "dependencies": {
    "dateformat": "^5.0.3",
    "dotenv": "^16.0.1"
  }
}